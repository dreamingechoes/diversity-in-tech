<h2>Show Company</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @company.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @company.description %>
  </li>

  <li>
    <strong>Logo:</strong>
    <%= @company.logo %>
  </li>

  <li>
    <strong>Website:</strong>
    <%= @company.website %>
  </li>

</ul>

<span><%= link "Edit", to: company_path(@conn, :edit, @company) %></span>
<span><%= link "Back", to: company_path(@conn, :index) %></span>
